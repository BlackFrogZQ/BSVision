    width: 10px;
    border-radius: 2px;
}

QSlider::handle:vertical {
    background: rgb(210, 210, 210);
    height: 20px;
    border-radius: 10px;

    margin-left: -6px;
    margin-right: -6px;
    margin-top: 2px;
    margin-bottom: 1px;
}

